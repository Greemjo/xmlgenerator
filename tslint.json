{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-console": false,
    "no-shadowed-variable": false,
    "object-literal-key-quotes": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "variable-name": false
  }
}
